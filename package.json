{
  "name": "graphlot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clear": "rm -rf .next",
    "format": "bun lint --fix --dir src",
    "add:ui": "bunx --bun shadcn@latest add",
    "db:local": "turso dev --db-file ./drizzle/local-db.db",
    "db:generate": "drizzle-kit generate",
    "db:apply": "bun run scripts/migrate.ts",
    "db:generate:dbml": "bun run src/db/generate-dbml.ts",
    "db:studio": "drizzle-kit studio",
    "db:reset": "rm -rf ./drizzle && bun db:generate && bun db:apply",
    "dev:tmux": "./scripts/tmux.sh",
    "codegen:hono": "bun scripts/hono-codegen.ts",
    "codegen:react-query": "bun scripts/react-query-codegen.ts",
    "codegen": "bun run codegen:hono && bun run codegen:react-query"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.2",
    "@libsql/client": "^0.15.10",
    "@notionhq/client": "^4.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.83.0",
    "better-auth": "^1.3.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "hono": "^4.8.9",
    "jotai": "^2.12.5",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.6",
    "next": "15.4.2",
    "next-themes": "^0.4.6",
    "pluralize": "^8.0.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.1.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "24.0.15",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-dbml-generator": "^0.10.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "fast-glob": "^3.3.3",
    "glob": "^11.0.3",
    "tailwindcss": "^4",
    "ts-morph": "^26.0.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3"
  }
}